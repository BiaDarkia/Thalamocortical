save_window_.size(0,650,-90,40)
scene_vector_[2] = save_window_
min_x_coord=0
max_x_coord=500
min_y_coord=-80
max_y_coord=120
{save_window_.view(min_x_coord, min_y_coord, max_x_coord, max_y_coord, 1, 75, 860, 699)}
graphList[0].append(save_window_)
