# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation

target: Cell3-supbask-FigA2a/Cell3-supbask-FigA2a_default/LEMS_Target.xml 
engine: jNeuroML_NEURON

mep: ./.test.supbaskFigA2a.mep
experiments:
  supbaskFigA2a:
    observables:
      spike times:
        file: 
          path: Cell3-supbask-FigA2a/Cell3-supbask-FigA2a_default/Sim_Target.CGsupbask.v.dat
          columns: [0,1]
          scaling: [1, 1]
        spike detection: 
          method: threshold
          threshold: -0.02
        tolerance: 0.001
