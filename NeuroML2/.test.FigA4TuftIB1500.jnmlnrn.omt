# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation

target: Cell7-tuftIB-FigA4-1500/Cell7-tuftIB-FigA4-1500_default/LEMS_Target.xml 
engine: jNeuroML_NEURON

mep: ./.test.tuftIBFigA4_1500.mep
experiments:
  tuftIBFigA4_1500:
    observables:
      spike times:
        file: 
          path: Cell7-tuftIB-FigA4-1500/Cell7-tuftIB-FigA4-1500_default/Sim_Target.CGtuftIB.v.dat
          columns: [0,1]
          scaling: [1, 1]
        spike detection: 
          method: threshold
          threshold: -0.02
        tolerance: 0.001
