# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation

target: Cell4-supaxax-FigA2a/Cell4-supaxax-FigA2a_default/LEMS_Target.xml 
engine: jNeuroML_NEURON

mep: ./.test.supaxaxFigA2a.mep
experiments:
  supaxaxFigA2a:
    observables:
      spike times:
        file: 
          path: Cell4-supaxax-FigA2a/Cell4-supaxax-FigA2a_default/Sim_Target.CGsupaxax.v.dat
          columns: [0,1]
          scaling: [1, 1]
        spike detection: 
          method: threshold
          threshold: -0.02
        tolerance: 0.001
