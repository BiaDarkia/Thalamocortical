# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation

target: Cell2-suppyrFRB-FigA1FRB/Cell2-suppyrFRB-FigA1FRB_default/LEMS_Target.xml 
engine: jNeuroML_NEURON

mep: ./.test.FigA1FRB.mep
experiments:
  FigA1FRB:
    observables:
      spike times:
        file: 
          path: Cell2-suppyrFRB-FigA1FRB/Cell2-suppyrFRB-FigA1FRB_default/Sim_Target.CGsuppyrFRB.v.dat
          columns: [0,1]
          scaling: [1, 1]
        spike detection: 
          method: threshold
          threshold: -0.02
        tolerance: 0.001
